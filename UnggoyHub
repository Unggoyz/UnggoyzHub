local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Unggoyz Hub | Unggoy#3120",
   LoadingTitle = "Welcome! | Unggoyz Hub",
   LoadingSubtitle = "Join https://discord.gg/dcfGHcv5 For Key",
   ConfigurationSaving = {
      Enabled = false,
      FolderName = nil, 
      FileName = "Unggoyz Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "hdcfGHcv5", 
      RememberJoins = true 
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Unggoyz Hub",
      Subtitle = "KEY resets weekly",
      Note = "KEY - https://discord.gg/dcfGHcv5 ",
      FileName = "Unggoyz KEY", 
      SaveKey = true, 
      GrabKeyFromSite = false, 
      Key = {"Monkey34"} 
   }
})


--Tabs
local MainTab = Window:CreateTab("Main", 9792650361)

local AdminTab = Window:CreateTab("Admin", 9792631281)

local PlayerTab = Window:CreateTab("Player", 9792631906)

local CreditTab = Window:CreateTab("Credits", 9792634075)
--sections
local SectionMain = MainTab:CreateSection("Main Scripts")

local SectionAdmin = AdminTab:CreateSection("Admin Scripts")

local SectionPlayer = PlayerTab:CreateSection("Local Player")

local SectionCredit = CreditTab:CreateSection("Credits")
--functions
function InfYield()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end

function AirHub()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Exunys/AirHub/main/AirHub.lua"))()
end

function WheelHub()
	loadstring(game:HttpGet('https://scripts.luawl.com/15362/KeshHub.lua'))()
end

function FatesEsp()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/fatesc/fates-esp/main/main.lua'))()
end

function CMDX()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
end

function FatesAdmin()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/fatesc/fates-admin/main/main.lua"))()
end

function Proxima()
